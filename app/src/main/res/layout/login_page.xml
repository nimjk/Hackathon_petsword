<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9D965">

    <ImageButton
        android:id="@+id/add_equip"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_x="141dp"
        android:layout_y="430dp"
        android:background="#00FFFFFF"
        android:src="@drawable/add_equipment" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="176dp"
        android:layout_y="242dp"
        android:src="@drawable/locker" />

    <ImageButton
        android:id="@+id/fingerprint"
        android:layout_width="wrap_content"
        android:layout_height="54dp"
        android:layout_x="45dp"
        android:layout_y="517dp"
        android:background="#00FFFFFF"
        android:src="@drawable/unlock_fingerprint" />

    <ImageButton
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="53dp"
        android:layout_x="109dp"
        android:layout_y="605dp"
        android:background="#00FFFFFF"
        android:src="@drawable/unlock_password" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="120dp"
        android:layout_y="166dp"
        app:srcCompat="@drawable/login_logo" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="154dp"
        android:layout_y="345dp"
        app:srcCompat="@drawable/ask_first_" />

</AbsoluteLayout>